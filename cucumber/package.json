{
  "name": "cucumber",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "wdio": "wdio run ./wdio.conf.js",
    "wdio:smoke": "set TAGS=@smoke&& wdio run ./wdio.conf.js",
    "wdio:regression": "set TAGS=@regression&& wdio run ./wdio.conf.js",
    "wdio:auth": "wdio run ./wdio.conf.js --spec ./features/auth.feature",
    "wdio:browse": "wdio run ./wdio.conf.js --spec ./features/browse.feature",
    "wdio:product": "wdio run ./wdio.conf.js --spec ./features/product.feature",
    "wdio:cart": "wdio run ./wdio.conf.js --spec ./features/cart.feature",
    "wdio:profile": "wdio run ./wdio.conf.js --spec ./features/profile.feature"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^9.24.0",
    "@wdio/cli": "^9.24.0",
    "@wdio/cucumber-framework": "^9.24.0",
    "@wdio/local-runner": "^9.24.0",
    "@wdio/spec-reporter": "^9.24.0"
  }
}
