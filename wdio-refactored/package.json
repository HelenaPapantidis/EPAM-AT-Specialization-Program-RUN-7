{
  "name": "wdio-refactored",
  "version": "1.0.0",
  "description": "WebdriverIO Test Automation Framework with Page Object Model",
  "type": "module",
  "scripts": {
    "test": "wdio run ./wdio.conf.js",
    "test:auth": "wdio run ./wdio.conf.js --spec=test/auth.test.js",
    "test:browse": "wdio run ./wdio.conf.js --spec=test/browse.test.js",
    "test:cart": "wdio run ./wdio.conf.js --spec=test/cart.test.js",
    "test:product": "wdio run ./wdio.conf.js --spec=test/productDetails.test.js",
    "test:profile": "wdio run ./wdio.conf.js --spec=test/profile.test.js"
  },
  "keywords": [
    "webdriverio",
    "test",
    "automation",
    "pom",
    "page-object-model"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wdio/cli": "^9.23.2",
    "@wdio/local-runner": "^9.23.2",
    "@wdio/mocha-framework": "^9.23.2",
    "@wdio/spec-reporter": "^9.23.2",
    "chai": "^6.2.2"
  },
  "dependencies": {
    "dotenv": "^17.2.4"
  }
}
